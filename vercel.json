{
  "builds": [
    {
      "src": "src/main/java/com/udea/lab1As/Lab1AsApplication.java",
      "use": "@vercel/java"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "production"
  }
}
